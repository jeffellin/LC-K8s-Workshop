apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: minimal-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: pi.${SESSION_NAMESPACE}.${INGRESS_DOMAIN}
    http:
      paths:
      - backend:
          service:
            name: pi-web-internal
            port:
              number: 80
        path: /
        pathType: Prefix